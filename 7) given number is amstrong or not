#include<stdio.h>
void main()
{
    int n,rem,s=0,t;
    printf("enter a number");
    scanf("%d",&n);
    t=n;
    while(n>0)
    {
        rem=n%10;
        s=s+rem*rem*rem;
        n=n/10;
    }
    if(t==s)
    {
        printf("the given number is amstrong");
        
    }
    else
    {
        printf("the given number is not amstrong");
    }
}
